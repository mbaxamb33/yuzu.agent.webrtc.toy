# Server
PORT=8080

# Daily.co (required)
DAILY_API_KEY=your-daily-api-key
DAILY_DOMAIN=yourteam.daily.co
DAILY_ROOM_PREFIX=ai-interview-
DAILY_ROOM_PRIVACY=private

# Daily room/token audio (optional overrides)
# High-fidelity bot audio recommended values shown
DAILY_ENABLE_MUSIC_MODE=true
DAILY_AUDIO_BITRATE=96000
DAILY_ENABLE_DTX=false
DAILY_ENABLE_PREJOIN_UI=true
DAILY_ENABLE_NETWORK_UI=true
DAILY_ENABLE_NOISE_CANCEL_UI=true

# Bot worker (required to start bots)
BOT_WORKER_CMD=/path/to/your/bot

# ElevenLabs (optional, for TTS)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_VOICE_ID=your-voice-id
